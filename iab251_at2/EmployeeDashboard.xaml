<Window x:Class="iab251_at2.EmployeeDashboard"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Employee Dashboard" Height="400" Width="600"
        WindowStartupLocation="CenterScreen">
    
    <Grid>
        <TextBlock Text="Employee Dashboard" FontSize="24" FontWeight="Bold" HorizontalAlignment="Center" Margin="0, 10, 0, 20"/>

        <Button Content="View Quotation Requests" Click="ViewQuotationsButton_Click" Width="200" Height="50" HorizontalAlignment="Center" VerticalAlignment="Center"/>
        <Button Content="Logout" Click="LogoutButton_Click" Width="100" Height="30" HorizontalAlignment="Right" VerticalAlignment="Top" Margin="10"/>
    </Grid>
</Window>
